    "autoload-dev": {
        "psr-4": {
            "Horde\\Turba\\Test\\": "test/"
        }
    },
    "bin": [
        "bin/turba-convert-datatree-shares-to-sql",
        "bin/turba-convert-sql-shares-to-sqlng",
        "bin/turba-import-openxchange",
        "bin/turba-import-squirrelmail-file-abook",
        "bin/turba-import-squirrelmail-sql-abook",
        "bin/turba-import-vcards",
        "bin/turba-public-to-horde-share",
        "bin/turba-validate-email"
    ],
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}